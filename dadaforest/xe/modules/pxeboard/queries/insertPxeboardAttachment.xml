<query id="insertPxeboardAttachment" action="insert">
    <tables>
        <table name="pxeboard_attachment" />
    </tables>
    <columns>
        <column name="module_srl" var="module_srl" filter="number" notnull="notnull" />
        <column name="document_srl" var="document_srl" filter="number" notnull="notnull" />
        <column name="file_srl" var="file_srl" filter="number" default="0" />
        <column name="screenshot_file_srl" var="screenshot_file_srl" filter="number" default="0" />
        <column name="screenshot_url" var="screenshot_url" default="" />
        <column name="screenshot_link" var="screenshot_link" default="" />
        <column name="preview_file_srl" var="preview_file_srl" filter="number" default="0" />
        <column name="preview_url" var="preview_url" default="" />
        <column name="label_file_srl" var="label_file_srl" filter="number" default="0" />
        <column name="label_url" var="label_url" default="" />
        <column name="downloaded" var="downloaded" size="11" default="0" />
        <column name="regdate" var="regdate" default="curdate()" />
    </columns>
</query>
