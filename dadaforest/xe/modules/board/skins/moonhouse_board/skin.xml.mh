    <date>2010-08-01</date>

    <author email_address="mbg1346@hotmail.com" link="http://moonhouse.co.kr">
        <name xml:lang="ko">80san</name>
        <name xml:lang="jp">80san</name>
        <name xml:lang="en">80san</name>
    </author>

    <colorset>
        <color name="white" src="screenshot/white.gif">
            <title xml:lang="ko">은색(기본)</title>
            <title xml:lang="jp">白（デフォルト）</title>
            <title xml:lang="en">White (default)</title>
        </color>
        <color name="cyan" src="screenshot/cyan.gif">
            <title xml:lang="ko">하늘색</title>
            <title xml:lang="jp">青緑</title>
            <title xml:lang="en">Cyan</title>
        </color>
        <color name="green" src="screenshot/green.gif">
            <title xml:lang="ko">초록색</title>
            <title xml:lang="jp">緑</title>
            <title xml:lang="en">Green</title>
        </color>
        <color name="red" src="screenshot/red.gif">
            <title xml:lang="ko">빨간색</title>
            <title xml:lang="jp">赤</title>
            <title xml:lang="en">Red</title>
        </color>
        <color name="orange" src="screenshot/orange.gif">
            <title xml:lang="ko">연분홍색</title>
            <title xml:lang="jp">桜色</title>
            <title xml:lang="en">orange</title>
        </color>
        <color name="black" src="screenshot/black.gif">
            <title xml:lang="ko">검정색</title>
            <title xml:lang="jp">黒い色</title>
            <title xml:lang="en">black</title>
        </color>
    </colorset>

    <extra_vars>
      <group>
        <title xml:lang="ko">세부 정보</title>
        <title xml:lang="jp">細部情報</title>
        <title xml:lang="en">details info</title>

        <var name="default_style" type="select">
            <title xml:lang="ko">기본 형태</title>
            <title xml:lang="jp">デフォルトスタイル</title>
            <title xml:lang="en">Default Form</title>
            <description xml:lang="ko">
                목록형, 웹진형, 갤러리형, 포럼형의 기본 스타일을 지정할 수 있습니다.
                포럼형 스타일은 정렬대상을 updated로 하셔야 합니다.
            </description>
             <description xml:lang="jp">
                リスト型、ウェブジン型、ギャラリー型、フォーラム型の基本スタイルを指定することが出来ます。
                フォーラム型のスタイルはソート対象を「updated」にしなければなりません。
            </description>
            <description xml:lang="en">
                You may select default styles such as list style, webzine style, gallery stylz or forum style.
                Align Target for forum style is required to be selected as 'updated'.
            </description>
            <options value="list">
                <title xml:lang="ko">목록</title>
                <title xml:lang="en">list</title>
                <title xml:lang="jp">リスト型</title>
            </options>
            <options value="webzine">
                <title xml:lang="ko">웹진</title>
                <title xml:lang="en">webzine</title>
                <title xml:lang="jp">ウェブジン(webzine)型</title>
            </options>
            <options value="gallery">
                <title xml:lang="ko">갤러리</title>
                <title xml:lang="en">gallery</title>
                <title xml:lang="jp">ギャラリー型</title>
            </options>
            <options value="bookmark">
                <title xml:lang="ko">북마크</title>
                <title xml:lang="en">bookmark</title>
                <title xml:lang="jp">ブックマーク</title>
            </options>
            <options value="studio">
                <title xml:lang="ko">스튜디오</title>
                <title xml:lang="en">studio</title>
                <title xml:lang="jp">写真館</title>
            </options>
            <options value="forum">
                <title xml:lang="ko">포럼</title>
                <title xml:lang="en">forum</title>
                <title xml:lang="jp">フォラム型</title>
            </options>
            <options value="blog">
                <title xml:lang="ko">블로그</title>
                <title xml:lang="en">blog</title>
                <title xml:lang="jp">ブログ型</title>
            </options>
        </var>
        <var name="board_info" type="select">
            <title xml:lang="ko">게시판상단 정보 출력</title>
			<title xml:lang="en">Top Info of Board</title>
			<title xml:lang="jp">掲示板上端情報出力</title>
            <options value="Y">
                <title xml:lang="ko">출력</title>
                <title xml:lang="en">display</title>
				<title xml:lang="jp">表示</title>
            </options>
            <options value="N">
                <title xml:lang="ko">출력하지 않음</title>
				<title xml:lang="en">not display</title>
				<title xml:lang="jp">非表示</title>
            </options>
            <description xml:lang="ko">게시판상단의 글수/로그인/설정표시의 출력유무
			단 관리자는 항상 보입니다.</description>
			<description xml:lang="jp">掲示板上端の文書数/ログイン/設定表示の出力有無
			ただし管理者は常に見えます.</description>
        </var>
        <var name="display_login_info" type="select">
            <title xml:lang="ko">　┖ 로그인 정보 출력</title>
            <title xml:lang="jp">ログイン情報表示</title>
            <title xml:lang="en">Display Login Info</title>
            <options value="Y">
                <title xml:lang="ko">출력</title>
                <title xml:lang="en">display</title>
                <title xml:lang="jp">表示</title>
            </options>
            <options value="N">
                <title xml:lang="ko">출력하지 않음</title>
                <title xml:lang="en">not display</title>
                <title xml:lang="jp">非表示</title>
            </options>
            <description />
        </var>
        <var name="display_setup_button" type="select">
            <title xml:lang="ko">　┖ 설정 표시</title>
            <title xml:lang="jp">設定表示</title>
            <title xml:lang="en">Display Setup</title>
            <options value="Y">
                <title xml:lang="ko">출력</title>
                <title xml:lang="en">display</title>
                <title xml:lang="jp">表示</title>
            </options>
            <options value="N">
                <title xml:lang="ko">출력하지 않음</title>
                <title xml:lang="en">not display</title>
                <title xml:lang="jp">非表示</title>
            </options>
            <description />
        </var>
        <var name="title" type="text">
            <title xml:lang="ko">게시판 제목</title>
            <title xml:lang="jp">掲示板タイトル</title>
            <title xml:lang="en">Title of Board</title>
            <description xml:lang="ko">게시판의 제목을 적어주세요.</description>
            <description xml:lang="jp">掲示板タイトルを入力して下さい。</description>
            <description xml:lang="en">Plase input the title of board.</description>
        </var>
        <var name="sub_title" type="text">
            <title xml:lang="ko">게시판 부제목</title>
            <title xml:lang="jp">掲示板サブタイトル</title>
            <title xml:lang="en">Subtitle of Board</title>
            <description xml:lang="ko">게시판 제목 옆에 나타날 부제목을 적어주세요.</description>
            <description xml:lang="jp">掲示板タイトルの隣りに表示されるサブタイトルを入力して下さい。</description>
            <description xml:lang="en">Please input the subtitle of board which will be displayed beside of board title.</description>
        </var>
        <var name="title_color" type="text" default="white">
            <title xml:lang="ko">　┖ 게시판 제목/부제목 색상</title>
            <title xml:lang="en">Title/SubTitle of Board</title>
            <title xml:lang="jp">掲示板サブタイトル カラー</title>
            <description xml:lang="ko">게시판의 제목과 부제목의 색상를 적어주세요.(기본색상 금색 #a98a60)</description>
            <description xml:lang="en">Title/SubTitle of Board Color(Default Gold #a98a60)</description>
            <description xml:lang="jp">掲示板サブタイトル カラー(Default Gold #a98a60)</description>
        </var>
        <var name="comment" type="textarea">
            <title xml:lang="ko">게시판 상세 설명</title>
            <title xml:lang="jp">掲示板の詳細</title>
            <title xml:lang="en">Description of Board</title>
            <description xml:lang="ko">게시판 제목 아래 표시될 설명을 입력하실 수 있습니다.</description>
            <description xml:lang="jp">掲示板タイトルの下に表示される説明文を入力して下さい。</description>
            <description xml:lang="en">You may input description which will be displayed under the board title.</description>
        </var>
        <var name="comment_color" type="text" default="white">
            <title xml:lang="ko">　┖ 게시판 상세 설명 색상</title>
            <title xml:lang="en">Description of Board Color</title>
            <title xml:lang="jp">掲示板の詳細 カラー</title>
            <description xml:lang="ko">게시판의 상세 설명의 색상적어주세요.(기본색상 금색 #a98a60)</description>
            <description xml:lang="en">Description of Board Color.(Default Gold #a98a60)</description>
            <description xml:lang="jp">掲示板の詳細 カラー(Default Gold #a98a60)</description>
        </var>
        <var name="subject_cut_size" type="text">
            <title xml:lang="ko">제목 글자수</title>
            <title xml:lang="jp">タイトルの文字数</title>
            <title xml:lang="en">Length of Subject</title>
            <description xml:lang="ko">제목 글자수를 지정할 수 있습니다. (0또는 비워주시면 자르지 않습니다)</description>
            <description xml:lang="jp">タイトルの文字数を設定します（「0」または空欄の場合は、文字数を制限しません）。</description>
            <description xml:lang="en">You may set length of title. (0 or blank value will not restrict the length)</description>
        </var>
        <var name="content_cut_size" type="text">
            <title xml:lang="ko">내용 글자수</title>
            <title xml:lang="jp">内容の文字数</title>
            <title xml:lang="en">Length of Content</title>
            <description xml:lang="ko">내용 글자수를 지정할 수 있습니다. (기본 240)</description>
            <description xml:lang="jp">内容の文字数を指定します(デフォルト 240)。</description>
            <description xml:lang="en">Length of Content can be assigned. (default 240)</description>
        </var>
        <var name="header_title_format" type="select">
            <title xml:lang="ko">게시판 제목 형식</title>
            <title xml:lang="jp">掲示板タイトルフォーマット</title>
            <title xml:lang="en">Header_title_format</title>
            <options value="h3">
                <title xml:lang="ko">H3</title>
                <title xml:lang="en">H3</title>
                <title xml:lang="jp">H3</title>
            </options>
			<options value="h1">
                <title xml:lang="ko">H1</title>
                <title xml:lang="en">H1</title>
                <title xml:lang="jp">H1</title>
            </options>
            <options value="h2">
                <title xml:lang="ko">H2</title>
                <title xml:lang="en">H2</title>
                <title xml:lang="jp">H2</title>
            </options>
            <options value="h4">
                <title xml:lang="ko">H4</title>
                <title xml:lang="en">H4</title>
                <title xml:lang="jp">H4</title>
            </options>
            <options value="h5">
                <title xml:lang="ko">H5</title>
                <title xml:lang="en">H5</title>
                <title xml:lang="jp">H5</title>
            </options>
            <options value="h6">
                <title xml:lang="ko">H6</title>
                <title xml:lang="en">H6</title>
                <title xml:lang="jp">H6</title>
            </options>
            <description />
        </var>
        <var name="document_title_format" type="select">
            <title xml:lang="ko">글 제목 형식</title>
            <title xml:lang="jp">書き込みタイトルフォーマット</title>
            <title xml:lang="en">Document_title_format</title>
            <options value="h4">
                <title xml:lang="ko">H4</title>
                <title xml:lang="en">H4</title>
                <title xml:lang="jp">H4</title>
            </options>
			<options value="h1">
                <title xml:lang="ko">H1</title>
                <title xml:lang="en">H1</title>
                <title xml:lang="jp">H1</title>
            </options>
            <options value="h2">
                <title xml:lang="ko">H2</title>
                <title xml:lang="en">H2</title>
                <title xml:lang="jp">H2</title>
            </options>
            <options value="h3">
                <title xml:lang="ko">H3</title>
                <title xml:lang="en">H3</title>
                <title xml:lang="jp">H3</title>
            </options>
            <options value="h5">
                <title xml:lang="ko">H5</title>
                <title xml:lang="en">H5</title>
                <title xml:lang="jp">H5</title>
            </options>
            <options value="h6">
                <title xml:lang="ko">H6</title>
                <title xml:lang="en">H6</title>
                <title xml:lang="jp">H6</title>
            </options>
            <description />
        </var>
        <var name="display_number" type="select">
            <title xml:lang="ko">번호 표시</title>
            <title xml:lang="jp">番号表示</title>
            <title xml:lang="en">Dispay Number</title>
            <options value="Y">
                <title xml:lang="ko">출력</title>
                <title xml:lang="en">display</title>
                <title xml:lang="jp">表示</title>
            </options>
            <options value="N">
                <title xml:lang="ko">출력하지 않음</title>
                <title xml:lang="en">not display</title>
                <title xml:lang="jp">非表示</title>
            </options>
            <description />
        </var>
        <var name="display_author" type="select">
            <title xml:lang="ko">글쓴이 표시</title>
            <title xml:lang="jp">投稿者表示</title>
            <title xml:lang="en">Display Author</title>
            <options value="Y">
                <title xml:lang="ko">출력</title>
                <title xml:lang="en">display</title>
                <title xml:lang="jp">表示</title>
            </options>
            <options value="N">
